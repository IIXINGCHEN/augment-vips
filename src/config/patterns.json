  "cleanup_modes": {
    "minimal": {
      "name": "Minimal Cleanup",
      "description": "Clean only basic trial data with lowest risk",
      "risk_level": "very_low",
      "backup_required": true,
      "confirmation_required": true,
      "operations": {
        "database_clean": {
          "enabled": true,
          "strategy": "trial_only",
          "patterns": ["trial_data"],
          "backup_before": true
        },
        "config_clean": {
          "enabled": false
        },
        "cache_clean": {
          "enabled": true,
          "strategy": "safe"
        }
      },
      "estimated_time_seconds": 30,
      "effectiveness_score": 60
    },
    
    "conservative": {
      "name": "Conservative Cleanup",
      "description": "Clean clearly safe data, suitable for cautious users",
      "risk_level": "low",
      "backup_required": true,
      "confirmation_required": true,
      "operations": {
        "database_clean": {
          "enabled": true,
          "strategy": "selective",
          "patterns": ["trial_data", "telemetry"],
          "backup_before": true
        },
        "config_clean": {
          "enabled": true,
          "strategy": "trial_only",
          "backup_before": true
        },
        "cache_clean": {
          "enabled": true,
          "strategy": "safe"
        }
      },
      "estimated_time_seconds": 60,
      "effectiveness_score": 75
    },
    
    "standard": {
